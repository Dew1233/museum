类别表
create table category(id int primary key auto_increment,name varchar(20)) charset=utf8;
insert into category values(null,'通知'),(null,'展览'),(null,'藏品'),(null,'服务'),(null,'知识'),(null,'活动');

create table announcement(id int primary key auto_increment,title varchar(50),resource varchar(50) ,acontent text,
viewCount int,category_id int,created bigint) charset = utf8;
